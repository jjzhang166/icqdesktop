QWidget#noContacts
{
	image: url(:/resources/placeholders/content_placeholder_cl_100.png);
	background-color: transparent;
	margin: 0;
}
QLabel#noContactsLabel
{
	color: #696969; font-size: 15dip;
	margin: 0; padding: 0;
	padding-left: 16dip; padding-right: 16dip; 
}

