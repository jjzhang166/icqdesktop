QWidget[ContactListWidget="true"]
{
	background: transparent;
}

QLineEdit#searchEdit
{
	font-size: 15dip;
	border-style: none;
}

QFrame#bottomPanel
{
	min-height: 72dip;
	background-color: #ebebeb;
	border-style: solid;
	border-width: 1dip;
	border-color: #dadada;
	border-left: none;
	border-bottom: none;
}

QPushButton#recentsButton
{
	min-width: 24dip;
	max-width: 24dip;
	min-height: 24dip;
	max-height: 24dip;
	border-image: url(:/resources/tabs_recents_100.png);
	margin-left: 24dip;
	margin-right: 24dip;
	margin-top: 24dip;
	margin-bottom: 24dip;
}
QPushButton#recentsButton:hover
{
	border-image: url(:/resources/tabs_recents_100_hover.png);
}
QPushButton#recentsButton:checked
{
	border-image: url(:/resources/tabs_recents_100_active.png);
}
QPushButton#recentsButton:checked:hover
{
	border-image: url(:/resources/tabs_recents_100_active.png);
}

QPushButton#liveChatsButton
{
	min-width: 24dip;
	max-width: 24dip;
	min-height: 24dip;
	max-height: 24dip;
	border-image: url(:/resources/tabs_livechats_100.png);
	margin-left: 24dip;
	margin-right: 24dip;
	margin-top: 24dip;
	margin-bottom: 24dip;
