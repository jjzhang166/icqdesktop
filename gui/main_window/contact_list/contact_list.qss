}

QLabel#liveChatsTitle
{
    text-align: left baseline;
    font-family: "%FONT_FAMILY_LIGHT%";
    font-weight: %FONT_WEIGHT_LIGHT%;
    font-size: 32dip;
    color: #282828;
    margin-left: 16dip;
    margin-bottom: 6dip;
    margin-top: 12dip;
}

